* {
    box-sizing: border-box;
}

body {
    margin: 0;
    overflow-x: hidden;
    width: 100%;
    font-family: 'Open Sans', sans-serif;
}

h1,
h2,
h3,
h4,
h5,
h6,
p {
    margin: 0;
    padding: 0;
}

ul {
    margin: 0;
    padding: 0;
}

li {
    list-style-type: none;
}

a {
    transition: all 0.2s ease-in-out;
    text-decoration: none;
    outline: none;
}

a:hover {
    text-decoration: none;
    color: inherit;
}

img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

section {
    padding: 40px 0;
}

.container {
    margin-right: auto;
    margin-left: auto;
    padding-left: 15px;
    padding-right: 15px;
    max-width: 1440px
}

.row {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    margin-right: -15px;
    margin-left: -15px;
}

.row:after {
    clear: both;
}

.col,
.col-6,
.col-4,
.col-8 {
    padding-right: 15px;
    padding-left: 15px;
    float: left;
    position: relative;
    min-height: 1px;
}

.col {
    width: 100%;
}

.col-6 {
    width: 50%;
}

.col-8 {
    width: 66.66666667%;
}

.col-4 {
    width: 33.33333333%;
}

figure,
img {
    margin: 0;
    padding: 0;
}

button {
    cursor: pointer;
}

.button {
    border: none;
    border-radius: 4px;
    background: #EF064E;
    color: #fff;
    transition: all .2s ease-in-out;
    outline: none;
    display: block;
    font-weight: 700;
    letter-spacing: 1px;
    padding: 10px 15px;
    text-align: center;
    min-width: 100px !important;
    text-decoration: none;
    -webkit-box-decoration-break: clone;
    -o-box-decoration-break: clone;
    box-decoration-break: clone;
}

.button:hover {
    background: #000;
}


/* HEADER CSS  */

.top-header {
    padding: 4px 0;
    background: #000;
}

.quick-contact {
    display: inline;
    text-align: left;
    padding: 7px 0;
}

.quick-contact span {
    padding: 0 !important;
}

.quick-contact span a {
    text-decoration: none;
    outline: none;
    color: #FFF;
    font-size: 12px;
}

.quick-contact span a:hover {
    color: #65A5A8;
}

.user-login {
    text-align: right;
}

.user-login a {
    outline: none;
    text-decoration: none;
    color: #EF064E;
    font-size: 14px;
    transition: all 0.2s ease-in-out;
}

.user-login a:nth-child(2) {
    padding-left: 8px;
}

.user-login a:hover {
    color: #65A5A8;
}


/* MENU BAR  */

.logo a {
    display: flex;
    align-items: center;
}

.logo img {
    height: 24px;
    margin-right: 5px;
    object-fit: unset;
    width: auto;
}

nav {
    text-align: right;
}

.navbar {
    background: #343a40;
}

.nav-header {
    padding: 15px 0;
}

.nav-header a {
    font-size: 16px;
    color: #FFF;
    text-decoration: none;
    padding: 0 8px;
}

.nav-header a:first-child {
    padding-left: 0;
}

.nav-header a:hover {
    color: #65A5A8;
}


/* BANNER SECTION  */

.banner-content {
    background-image: url(../assets/slider_1.jpg);
    background-size: cover;
    height: 500px;
    background-position: center;
    background-repeat: no-repeat;
}

.banner-content figure img {
    width: 100%;
}

.banner-content .caption {
    height: 500px;
    display: flex;
    flex-flow: column;
    justify-content: center;
    color: #fff;
    align-items: center;
    text-align: center;
}

.banner-content .caption h1 {
    line-height: 1;
    margin-bottom: 15px;
}

.banner-content .caption p {
    margin-bottom: 15px;
}

.home-banner {
    background: #f9f9f9;
}


/*Banner slider*/

.banner-slider .item {
    margin-right: 0px;
    border: 1px solid #EEE;
}

.banner-slider a figure {
    width: 100%;
    height: 400px;
    overflow: hidden;
    position: relative;
}

.banner-slider a figure img {
    width: 100%;
    height: auto;
}

.banner-slider a figure span {
    position: absolute;
    content: '';
    width: auto;
    height: auto;
    bottom: 20px;
    left: 20px;
    background: rgba(0, 0, 0, .7);
    padding: 10px;
    color: #FFF;
}

.banner-slider a figure span h3 {
    text-transform: capitalize;
    font-size: 30px;
    font-weight: 600;
    color: #65A5A8;
}

.banner-slider a figure span h3:hover {
    color: #82CCF8;
}

.banner-slider a figure span p {
    text-align: center;
    font-size: 18px;
    font-weight: 600;
    color: #DDD;
}

.banner-slider .owl-item figure img {
    transition: all 0.3s ease-in-out;
}

.banner-slider .owl-item figure img:hover {
    transform: scale(1.06);
    box-shadow: 0px 0px 3px 0px #f67a21;
}


/*Banner new products*/

.banner-new-product {
    width: 100%;
    box-shadow: 1px 1px 5px #666;
    position: relative;
}

.banner-new-product a figure {
    width: 100%;
    height: 400px;
    overflow: hidden;
}

.banner-new-product a figure img {
    width: 100%;
}

.banner-new-product a figure span {
    position: absolute;
    content: '';
    width: 100%;
    height: auto;
    bottom: 0px;
    left: 0px;
    background: rgba(255, 255, 255, .6);
    padding: 10px;
    color: #FFF;
    text-align: center;
}

.banner-new-product a figure span h3 {
    text-transform: capitalize;
    font-size: 30px;
    font-weight: 600;
    color: #000;
}

.banner-new-product a figure span h3:hover {
    color: #555;
}

.banner-new-product a figure span p {
    text-transform: capitalize;
    font-size: 14px;
    font-weight: 400;
    color: #000;
}


/* CAROUSEL CSS  */

.owl-carousel .owl-nav .owl-next,
.owl-carousel .owl-nav .owl-prev {
    padding: 0 10px;
    font-size: 20px;
    font-weight: 500;
    background: #65A5A8;
    color: #FFF;
    position: absolute;
    top: 45%;
    cursor: pointer;
    border: 0;
}

.owl-carousel .owl-nav .owl-prev {
    left: 0;
}

.owl-carousel .owl-nav .owl-next {
    right: 0;
}


/*TOP CATEGORIES*/

.multi-img-slider .item {
    margin-right: 0px;
    border: 0.01em solid #999;
}

.multi-img-slider a figure {
    width: 100%;
    height: 180px;
    overflow: hidden;
    position: relative;
}

.multi-img-slider a figure img {
    width: 100%;
    height: auto;
}

.multi-img-slider a figure span {
    position: absolute;
    content: '';
    width: 100%;
    height: auto;
    bottom: 0px;
    left: 0px;
    background: rgba(0, 0, 0, .7);
    padding: 10px;
    color: #FFF;
    text-align: center;
}

.multi-img-slider a figure span h3 {
    text-transform: capitalize;
    font-size: 20px;
    font-weight: 400;
    color: #65A5A8;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    overflow: hidden;
    -webkit-box-orient: vertical;
    transition: all 0.2s ease;
}

.multi-img-slider a figure span h3:hover {
    color: #FFF;
}

.multi-img-slider .owl-item figure img {
    transition: all 0.3s ease-in-out;
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.multi-img-slider .owl-item figure img:hover {
    transform: scale(1.06);
}


/*NEW ARRIVAL*/

.title-header {
    text-align: left;
    margin-bottom: 10px;
}

.title-header h2 {
    /*background: #000;*/
    text-transform: capitalize;
    font-size: 24px;
    padding-bottom: 10px;
    border-bottom: 0.01em solid #DDD;
}

.title-header h2 span {
    text-transform: capitalize;
    font-size: 14px;
    color: #666;
}

.see-all {
    text-align: right;
    padding: 2px 0;
    /*margin-bottom: 10px;*/
}

.see-all button {
    padding: 2px 10px;
    border: none;
    background: #EF064E;
    color: #FFF;
    transition: all 0.2s ease-in-out;
    outline: none;
}

.see-all button:hover {
    background: #000;
}

.image-text-block-wrapper {
    background: #f9f9f9;
}

.image-text-block-content {
    padding: 10px;
    background: #FFF;
    overflow: hidden;
    margin-bottom: 15px;
    border: 0.01em solid #CCC;
    transition: all 0.2s ease;
}

.image-text-block-content:hover {
    box-shadow: 0px 0px 3px 0px #999;
}

.image-text-block-content figure {
    width: 200px;
    height: 100px;
    overflow: hidden;
    float: left;
}

.image-text-block-content figure img {
    width: 100%;
}

.image-text-block-detail {
    display: table;
    padding: 10px 20px;
}

.image-text-block-detail h4 a {
    text-transform: capitalize;
    font-size: 18px;
    font-weight: 600;
    line-height: 24px;
    color: #444;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    overflow: hidden;
    -webkit-box-orient: vertical;
}

.image-text-block-detail p {
    font-size: 14px;
    font-weight: 400;
    padding: 4px 0;
    color: #444;
}

.image-text-block-detail p span {
    color: #00A9E0;
}

.image-text-block-detail a {
    font-size: 14px;
    font-weight: 400;
    padding: 4px 0;
    color: #65A5A8;
    text-decoration: none;
    outline: none;
}

.image-text-block-detail a i {
    padding-right: 5px;
    font-size: 16px;
}

.image-text-block-detail a:hover {
    color: #EF064E;
}


/* SLIDER WITH IMAGE GALLERY  */

.block-slider {
    padding: 30px 15px 30px;
    border: 0.01em solid #444;
}

.block-content {
    float: left;
    display: block;
    padding: 10px;
    margin-right: 15px;
    min-width: calc(50% - 15px);
    background: #FFF;
    overflow: hidden;
    margin-bottom: 15px;
    border: 0.01em solid #CCC;
    transition: all 0.2s ease;
}

.block-content:hover {
    box-shadow: 0px 0px 3px 0px #999;
}

.block-content figure {
    width: 150px;
    height: 100px;
    overflow: hidden;
    float: left;
}

.block-content figure img {
    width: 100%;
}

.block-content-detail {
    display: table;
    padding: 10px 20px;
}

.block-content-detail h4 a {
    text-transform: capitalize;
    font-size: 16px;
    font-weight: 400;
    line-height: 24px;
    color: #444;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    overflow: hidden;
    -webkit-box-orient: vertical;
}

.block-content-detail p {
    font-size: 14px;
    font-weight: 400;
    padding: 4px 0;
    color: #444;
}

.block-content-detail p span {
    color: #00A9E0;
}

.block-content-detail a {
    font-size: 14px;
    font-weight: 400;
    padding: 4px 0;
    color: #65A5A8;
    text-decoration: none;
    outline: none;
}

.block-content-detail a i {
    padding-right: 5px;
    font-size: 16px;
}

.block-content-detail a:hover {
    color: #EF064E;
}

.owl-carousel.block-slider-content .owl-nav .owl-prev {
    bottom: -20px;
    right: 40px;
    left: auto;
    top: auto;
}

.owl-carousel.block-slider-content .owl-nav .owl-next {
    bottom: -20px;
    right: 0;
    top: auto;
}


/*Side Gallery*/

.gallery-sidebar {
    background: #f9f9f9;
    text-align: center;
    border: 0.01em solid #666;
    margin-bottom: 20px;
}

.gallery-sidebar h3 {
    padding: 10px;
    font-size: 20px;
    font-weight: 300;
    text-transform: capitalize;
    color: #FFF;
    background: #2F2E2E;
}

.gallery-sidebar-content {
    text-align: center;
    padding-top: 5px;
}

.gallery-sidebar-content ul {
    text-align: center;
}

.gallery-sidebar-content ul li {
    display: inline-block;
    margin: 4px;
    width: calc(50% - 12px);
    height: 90px;
    overflow: hidden;
    position: relative;
}

.gallery-sidebar-content ul li a img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.gallery-sidebar-content ul li a span {
    visibility: hidden;
    position: absolute;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, .3);
    opacity: 0;
    transition: all 0.3s ease-in-out;
}

.gallery-sidebar-content ul li a span i {
    position: absolute;
    font-size: 26px;
    top: 40%;
    left: 40%;
    color: #FFF;
    opacity: 1;
    transition: all 0.1s ease-in-out;
}

.gallery-sidebar-content ul li a span i:hover {
    color: #666;
}

.gallery-sidebar-content ul li a:hover span {
    visibility: visible;
    opacity: 1;
}


/* Rating Slider */

.fl-kids-wear-wrapper {
    padding: 40px 0;
    background: #f9f9f9;
}

.rating-slider-content {
    border: 0.01em solid #AAA;
    text-align: center;
    background: #FFF;
}

.review-slider .owl-stage-outer {
    padding-bottom: 50px;
}

.rating-slider-content figure {
    width: 100%;
    height: 140px;
    overflow: hidden;
    position: relative;
}

.rating-slider-content figure img {
    width: 100%;
    height: auto;
}

.rating-slider-content figure span {
    position: absolute;
    content: '';
    visibility: hidden;
    opacity: 0;
    width: 100%;
    height: 100%;
    top: 0px;
    left: 0px;
    background: rgba(0, 0, 0, .7);
    transition: all 0.4s ease;
    /*text-align: center;*/
}

.rating-slider-content figure span h3 {
    position: absolute;
    top: calc(50% - 15px);
    left: calc(50% - 40px);
    transition: all 0.2s ease;
}

.rating-slider-content figure span h3 i {
    font-size: 30px;
    color: #FFF;
    padding: 5px;
    transition: all 0.2s ease;
}

.rating-slider-content figure span h3 i:hover {
    color: #666;
}

.rating-slider-content figure:hover span {
    visibility: visible;
    opacity: 1;
}

.rating-slider-detail {
    padding: 10px 0;
}

.rating-slider-detail h4 {
    text-transform: capitalize;
    font-size: 18px;
    font-weight: 600;
    line-height: 24px;
    color: #444;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    overflow: hidden;
    -webkit-box-orient: vertical;
}

.rating-slider-detail h5 i {
    color: #FFC300;
    padding: 4px 0;
}

.rating-slider-detail p {
    font-size: 14px;
    font-weight: 400;
    color: #444;
}

.rating-slider-content p span {
    color: #00A9E0;
}


/* ICON TEXT GRID  */

.section-header {
    text-align: center;
}

.section-header h2 {
    text-transform: uppercase;
    font-size: 34px;
    padding-bottom: 20px;
    border-bottom: 0.01em solid #DDD;
}

.icon-text-content {
    padding-top: 20px;
}

.icon-text-content ul {
    text-align: center;
}

.icon-text-content ul li {
    display: inline-block;
    max-width: 275px;
    padding: 0 10px;
    margin-bottom: 20px;
}

.icon-text-content ul li i {
    font-size: 30px;
    color: #EF064E;
    border: 1px solid;
    padding: 20px;
    border-radius: 60%;
}

.icon-text-content ul li h4 {
    text-transform: capitalize;
    font-size: 26px;
    padding: 10px 0;
}

.icon-text-content ul li p {
    font-size: 14px;
    color: #777;
}


/*REVIEW*/

.review-block {
    padding: 40px 0;
    background: #f9f9f9;
}

.author-detail {
    float: left;
}

.author-detail figure {
    width: 100px;
    height: 100px;
    overflow: hidden;
    border-radius: 60%;
    margin: 20px auto 20px;

    border: 2px solid;
}

.author-detail figure img {
    width: 100%;
        object-fit: cover;
    height: 100%;
}

.author-detail h2 {
    text-align: center;
    /*margin-bottom: 10px;*/
}

.author-detail h2 span {
    font-size: 20px;
    color: #000;
    font-weight: 400;
    padding: 10px 0;
    position: relative;
    /*border-top: 2px solid;*/
    /*background: #ff6900;*/
}

.author-detail h2 span:before {
    position: absolute;
    content: '';
    left: calc(50% - 25px);
    top: 0px;
    width: 50px;
    height: 2px;
    background: #666;
}

.author-detail p {
    text-align: center;
    font-size: 13px;
    color: #666;
    font-weight: 400;
    text-transform: uppercase;
    /*padding: 5px 10px;*/
    /*border-top: 2px solid;*/
    /*background: #ff6900;*/
}

.author-detail ul {
    text-align: center;
}

.author-detail ul li {
    display: inline-block;
}

.author-detail ul li a i {
    width: 26px;
    height: 26px;
    padding: 5px;
    color: #000;
    font-size: 16px;
    transition: all 0.2s ease;
}

.author-detail ul li a i:hover {
    background: #000;
    color: #FFF;
}

.review-message {
    display: table;
}

.review-message p {
    font-size: 16px;
    color: #000;
    font-weight: 300;
    padding: 10px;
    margin: 20px 10px;
    letter-spacing: 0.04em;
    /*border-radius: 10px;*/
}

.owl-carousel.review-slider .owl-nav .owl-prev {
    left: calc(50% - 30px);
    bottom: 0;
    top: auto;
}

.owl-carousel.review-slider .owl-nav .owl-next {
    right: calc(50% - 30px);
    bottom: 0;
    top: auto;
}
/* ========================
FOOTER 
======================== */
.main-footer {
    background: #343a40;
}
.footer-links ul li {
    margin: 5px 0;
}
.footer-links ul li a {
    color: #FFF;
}

/* ========================
RESPONSIVE
======================== */

@media (max-width:1199px) {}

@media (max-width:991px) {}

@media (max-width:767px) {
    .col,
    .col-4,
    .col-6,
    .col-8 {
        width: 100%;
    }
    .banner-slider a figure {
        height: auto;
        margin-bottom: 30px;
    }
    .image-text-block-content figure {
        width: 100%;
        height: 200px;
        float: unset;
    }
}